pipeline{

  agent any

  stages{
    stage('Checkout'){
        steps{
          checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/cosmeaf/django_budget']]])
      }
    }
  }

  stages{
    stage('Setup Python Virtual ENV'){
        steps{
          sh '''
            chmod +x setupEnviroment.sh
            ./setupEnviroment.sh
          '''
        }
    }
  }
  
}